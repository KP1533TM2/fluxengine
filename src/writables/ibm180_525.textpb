comment: 'PC 180kB 5.25" 40-track 9-sector single-sided'

input {
	image {
		filename: "ibm180_525.img"
		img {}
	}
}

output {
	flux {
		drive {}
	}
}

geometry {
	cylinders: 40
	heads: 1
	trackdata {
		sectors: 9
		sector_size: 512
	}
}

encoder {
	ibm {
		trackdata {
			track_length_ms: 167
			clock_rate_khz: 300
			sector_skew: "012345678"
		}
	}
}

cylinders {
	start: 0
	end: 79
}

heads {
	start: 0
	end: 0
}


comment: 'Brother 120kB 3.5" 39-track GCR disks'

input {
	image {
		filename: "brother120.img"
		img {}
	}
}

output {
	flux {
		drive {}
	}
}

geometry {
	cylinders: 39
	heads: 1
	trackdata {
		sectors: 12
		sector_size: 256
	}
}

encoder {
	brother {
		format: BROTHER120
	}
}

cylinders {
	start: 0
	end: 39
}

heads {
	start: 0
	end: 0
}


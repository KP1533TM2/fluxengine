comment: 'Kaypro II 5.25" single-sided 48 tpi'

image_reader {
	filename: "kaypro2.img"
	img {}
}

image_writer {
	filename: "kaypro2.img"
	img {
		trackdata {
			sector_size: 512
			sectors: 10
		}
		tracks: 40
		sides: 1
		physical_step: 1
	}
}

encoder {
	ibm {
		trackdata {
			track_length_ms: 195
			clock_rate_khz: 500
			sector_skew: "0123456789"
		}
	}
}

decoder {
	ibm {
		sector_id_base: 0
	}
}

cylinders {
	start: 0
	end: 39
}

heads {
	start: 0
	end: 0
}

